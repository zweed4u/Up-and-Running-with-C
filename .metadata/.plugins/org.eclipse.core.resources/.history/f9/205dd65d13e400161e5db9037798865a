#include <stdio.h>
#include <stdlib.h>

int main(void) {
	//* is multiply
	/// is division
	//++ increment
	//-- decrement
	// calculation on right, result on left/variable
	int a;
	int b=5;
	printf("Input an integer: ");
	scanf("%d",&a);
	printf("%d + %d = %d\n",a,b,a+b)
}
