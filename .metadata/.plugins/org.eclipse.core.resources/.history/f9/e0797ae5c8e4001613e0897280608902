#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>//string manipulation
int main(void) {
	int ch='a';
	printf("Big %c\n",toupper(ch));
	printf("Little %c\n",tolower(ch));
	printf("What begins with %c\n",ch);
}
