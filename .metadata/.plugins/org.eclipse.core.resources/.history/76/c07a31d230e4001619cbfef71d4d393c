#include <stdio.h>
#include <stdlib.h>

void blorf(void); //prototypes blorf

int main(void) {
	puts("The main() function always runs first");
	blorf();
	puts("Thanks blorf");
	return EXIT_SUCCESS;
}

void blorf(void){
	puts("This is output by the blorf function");
}
