#include <stdio.h>
#include <stdlib.h>
int iscaps(int ch);
int main(void) {
	int c;
	do{
		c=getchar();
		if(iscaps(c)){
			putchar(c);
		}
	}
	while(c!='\n');
	return(0);
}

int iscaps(int ch){
	if(ch<'A'||ch>'Z'){
		return(1);
	}
	else{
		return(0);
	}
}
