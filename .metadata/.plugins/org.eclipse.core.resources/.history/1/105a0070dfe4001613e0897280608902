#include <stdio.h>
#include <stdlib.h>

int main(void) {
	//pointers must be initialized!!!
	//pointer type must match the variable it references
	int pokey;
	int *p;
	p=&pokey;//intialize pointer
	printf("The address of `pokey` is: %p\n",&pokey);//& fetches address
	printf("The address of `pokey` is: %p\n",p);//address fetched with pointer

	return EXIT_SUCCESS;
}
