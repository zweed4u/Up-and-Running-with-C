char *longer(char *s1, char *s2);//returns character pointer (address)

int main(void) {
	char *string1="A long time ago";
	char *string2="In a galaxy far, far away";
	char *result;
	result = longer(string1,string2);
	printf("String '%s' is longer.\n",result);

	return EXIT_SUCCESS;
}
