#include <stdio.h>
#include <stdlib.h>

int main(void) {
	//array of pointers -- collection of mem addresses
	char *gang[5]={//little rascals
			"Spanky",
			"Buckwheat",
			"Alfalfa",
			"Darla",
			"Pete the Pup"
	};
	int x;
	for(x=0;x<5;x++){
			printf("%s\n",gang[x]);
	}
	return EXIT_SUCCESS;
}
