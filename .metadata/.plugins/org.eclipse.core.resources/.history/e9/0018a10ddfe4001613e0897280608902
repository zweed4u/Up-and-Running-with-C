#include <stdio.h>
#include <stdlib.h>

int main(void) {
	//pointers must be initialized!!!
	//pointer type must match the variable it references
	int pokey;
	int *p
	printf("The address of `pokey` is: %p\n",&pokey);//& fetches address
	return EXIT_SUCCESS;
}
