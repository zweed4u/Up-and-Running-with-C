#include <stdio.h>
#include <stdlib.h>
#include <time.h>

//preprocesser directors are not c language statements - no semicolon in defines(constants)
int main(void) {
	int row,column;
	srand((unsigned)time(NULL));//seed randomizer using tictoc
	for(row=0;row<10;row++){
		for(column=0;column<10;column++){
			printf("%2d\t",rand()%100);//0 to 99
		}
		putchar('\n');
	}
	return EXIT_SUCCESS;//same as 0
}
