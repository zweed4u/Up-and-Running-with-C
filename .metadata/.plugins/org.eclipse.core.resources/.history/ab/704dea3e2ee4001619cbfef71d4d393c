#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int x;
	for(x=0;x<10;x++){//initialization(assignment); looping condition; iteration statement(increment)
		printf("%d\n",x+1);
	}

	//1-20
	//evens 2-20
	//10-1

	int challenge;
	for(challenge=0;challenge<=2;challenge++){//3challenges
		for(int ch1=0;ch1<20;ch1++){
			printf("Challenge%d: %d\n",challenge+1,ch1+1);
		}

	}




	return EXIT_SUCCESS;
}

