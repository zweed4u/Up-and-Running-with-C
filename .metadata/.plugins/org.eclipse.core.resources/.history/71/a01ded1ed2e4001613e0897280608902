#include <stdio.h>
#include <stdlib.h>

int main(void) {
	//bad without arrays
	float temp1,temp2,temp3,temp4;
	temp1=84.9;
	temp2=83.7;
	temp3=85.8;
	temp4=88.2;
	printf("Local temperatures:\n");
	printf("Station 1: %.1f\n",temp1);
	printf("Station 2: %.1f\n",temp2);
	printf("Station 3: %.1f\n",temp3);
	printf("Station 4: %.1f\n",temp4);

	//using arrays
	float temps[4]={=84.9,83.7,85.8,88.2};



	return EXIT_SUCCESS;
}
