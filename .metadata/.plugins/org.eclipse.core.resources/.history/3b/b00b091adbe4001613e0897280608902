#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(void) {
	time_t now;//time datatype

	time(&now)
	printf("The current time is %ld\n",now);//placeholder ld is long integer value (epoch returned)

	return EXIT_SUCCESS;
}
