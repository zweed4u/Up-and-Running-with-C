#include <stdio.h>
#include <stdlib.h>

int main(void) {
	//* is multiply
	/// is division
	//++ increment
	//-- decrement
	// calculation on right, result on left/variable
	float a;
	float b=5.0;
	printf("Input a float: ");
	scanf("%f",&a);
	printf("%f / %f = %f\n",a,b,a/b);

	int x = 10;
	printf("%d\n",x);
	x++;
	printf("%d\n",x);


	int aa;
	aa=25/5*2+3; //shows precedence (duh)
	printf("The answer is: %d\n",aa);
	aa=25/(5*(2+3)); //shows precedence (duh)
	printf("The answer is: %d\n",aa);


	float r;
	r=sqrt(2.0);

	return(0);
}
